{
  "showStatus": true,
  "enableGlobDot": false,
  "enabledLanguageIds": [
    "markdown",
    "json"
  ],
  "features": {
    "weighted-suggestions": true
  },
  "globRoot": "${workspaceFolder}",
  "ignorePaths": [
    "${workspaceFolder}/.vscode/"
  ],
  "words": [
    "Blinn",
    "framebuffer",
    "mattgraham",
    "Phong",
    "uikit",
    "Vulkan"
  ],
  "ignoreWords": [
    "jmicheli",
    "subdir"
  ]
}
